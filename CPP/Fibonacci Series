/*Problem Statement: Fibonacci series upto certain number.
Input: 6
Output: 0 1 1 2 3 5
*/



#include <iostream>

using namespace std;


int main()
{
    int n,n1=0,n2=1,n3;
    std::cout << "Enter how many numbers you want to see in fibonacci series " << std::endl;
    std::cin >> n;
    cout<<n1<<" "<<n2<<" "; //printing 0 and 1    
    for(int i=2;i<n;++i) //loop starts from 2 because 0 and 1 are already printed    
   {    
     n3=n1+n2;    
     cout<<n3<<" ";    
     n1=n2;    
     n2=n3;    
   }    
    return 0;
}
